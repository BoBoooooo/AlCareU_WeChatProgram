<view class="page">
     <view class="no-order" hidden="{{orderList ? true : false}}">
        <image src="/images/no-order.png" class="no-order-img"></image>
        <view class="text">暂无体检报告</view>
    </view>
    <view class="page__bd">
      
        <view class="weui-cells weui-cells_after-title"  >
            <view  class="weui-cell weui-cell_access" hover-class="weui-cell_active" bindtap="orderDetail" data-id="{{item.ID}}" wx:for-items="{{orderList}}" wx:key="{{index}}" >
                <view class="weui-cell__bd">{{item.examstime}}  {{item.ReportName}}</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </view>
        
        </view>

    </view>
</view>