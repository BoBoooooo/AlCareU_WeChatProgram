            </view>
             <view class="order-date">
                <view class="date-box">预约号：{{item.sequence}}</view>
                <!-- <view class="status {{(item.status==-1 || item.status==4) ? '':'red'}}">{{item.statusStr}}</view> -->
            </view>
          
           
        </view>

    </view>

    <view style='position:fixed;bottom:0'>
                    <image bindtap='GetNo' src='/images/add-addr.png' style='width:65rpx;height:65rpx'></image>
    </view>
